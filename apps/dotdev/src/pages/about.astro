---
import MainLayout from "@/layouts/MainLayout";

const {
	entity: { pronouns },
} = DotDev;
---

<MainLayout>
	<article
		flex="~ center-start col-nowrap gap-4 grow-1"
		m="[&>*]:0"
		p="x-[clamp(1rem,3rem,5.5vw)] y-12"
		size="w-min-full"
	>
		<h1 text-align="center">About {pronouns.object == "it" ? "It" : "Me"}</h1>
		<p>I don't really know what to say here.</p>
	</article>
</MainLayout>
