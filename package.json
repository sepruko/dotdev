{
	"$schema": "https://json.schemastore.org/package",
	"name": "@sepruko/monorepo-dotdev",
	"version": "0.1.0",
	"description": "A monorepository for the sepruko.dev website.",
	"keywords": [
		"dotdev",
		"monorepo",
		"monorepository",
		"pnpm",
		"sepruko",
		"www"
	],
	"homepage": "https://github.com/sepruko/dotdev#readme",
	"bugs": "https://github.com/sepruko/dotdev/issues",
	"license": "MS-PL",
	"author": "Nat Sepruko <me@sepruko.dev> (https://sepruko.dev/)",
	"contributors": [],
	"files": [],
	"type": "module",
	"exports": {
		"./package.json": "./package.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sepruko/dotdev.git"
	},
	"scripts": {
		"check": "biome check . && prettier -c .",
		"check:fix": "biome check --write . && prettier -w .",
		"format": "biome format --write . && prettier -w .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@biomejs/biome": "~1.9.4",
		"@sepruko/shared-config": "~0.4.8",
		"husky": "~9.1.6",
		"prettier": "^3.3.3",
		"prettier-plugin-astro": "~0.14.1",
		"typescript": "~5.6.3"
	},
	"engines": {
		"node": ">=22.11.0",
		"pnpm": "^9.13.0"
	},
	"devEngines": {
		"node": ">=22.11.0",
		"pnpm": "^9.13.0"
	},
	"private": true,
	"pnpm": {
		"overrides": {
			"typescript": "$typescript"
		}
	}
}
